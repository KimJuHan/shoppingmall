<% include ../partials/header %>
<br>
<h3 style="text-align:center;">상품Q&A</h3>
<hr>
    <form action="/board/QnA/search" method="GET">
        검색어 : <input type="text" name="searchQnA">
        <input type="submit" value="찾기">
    </form>
    <table>
        <tr>
            <th>NO</th>
            <th>SUBJECT</th>
            <th>TITLE</th>
            <th>NAME</th>
            <th>DATE</th>
        </tr>
        <% QnAs.forEach(function(QnA){%>
            <tr>
                <td><%= QnA.id %></td>
                <% if(QnA.Product){%>
                    <td><img src="/uploads/<%= QnA.Product.dataValues.thumbnail%>" style="width:100px;height:100px;" alt=""><br><%= QnA.Product.dataValues.name%></td>            
                <%}%>
                <% if(QnA.category == 'delivery'){%>
                    <td><img src="data:image/svg+xml;utf8;base64," style="width:100px;height:100px;"/><br>배송문의</td>
                <%}%>
                <% if(QnA.category == 'exchange'){%>
                    <td><img src="data:image/svg+xml;utf8;base64," style="width:100px;height:100px;"/><br>교환/환불문의</td>
                <%}%>
                <td>
                    <a href="/board/QnA/<%=QnA.id%>">
                        <%= QnA.title %>
                        <% if(QnA.display == "off"){%>
                            <img src="//img.echosting.cafe24.com/design/skin/admin/ko_KR/ico_lock.gif" alt="">
                        <%}%>
                    </a>
                </td>
                <td><%= (QnA.User) ? (QnA.User.dataValues.username) : "" %></td>
                <td><%= QnA.createdAt %></td>
            </tr>
        <%})%>
    </table>
    <a type="button" href="/board/QnA/write">글쓰기</a>
<% include ../partials/footer %>